<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!--
        Added the entity AkipTaskInstanceRankingContextConfig.
    -->
    <changeSet id="00000000002002-1" author="akip">
        <createTable tableName="akip_ti_ranking_context_config">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="context_value_expression" type="${clobType}">
                <constraints nullable="true" />
            </column>
            <column name="process_definition_id" type="bigint">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
